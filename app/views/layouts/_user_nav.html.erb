<%= content_for :user_nav do %>

<% if current_user.class.to_s == 'Student' %>
<%= render "layouts/student_nav" %>
<% elsif current_user.class.to_s == 'Driver' %>
<%= render "layouts/driver_nav" %>
<% elsif current_user.class.to_s == 'Manager' %>
<%= render "layouts/manager_nav" %>
<% else%>
<%end%>
<% end %>